      "dp":    { "real": 15, "unit": "kPa" },
      "Ro": { "real": 0.72 },
      "Roc": { "real": 0.64 },
      "k":  { "real": 1.37 },
      "mu": { "real": 19.0, "unit": "uPa_s" }
    }
  },
  "lenPackage": {
    "lenProperties": {
      "d20": { "real": 200, "unit": "mm" },
      "D20":   { "real": 290, "unit": "mm" },
